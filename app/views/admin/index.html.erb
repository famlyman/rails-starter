<h1>Admin#index</h1>
<p>Find me in app/views/admin/index.html.erb</p>
<br />
<% @users.each do |user| %>
    <%= form_for(user, url: admin_path(user), remote: true, method: :patch) do |f| %>
        <p><%= f.text_field :email %> - <%= f.select(:role, User.roles.keys.map { |w| [w.humanize, w] }) %> - <%= f.submit "Update" %></p>
    <% end %>
<% end %>